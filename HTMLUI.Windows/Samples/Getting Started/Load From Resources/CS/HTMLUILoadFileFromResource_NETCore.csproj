<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>HTMLUILoadFileFromResource</AssemblyName>
    <RootNamespace>Syncfusion.Windows.Forms.HTMLUI.Samples</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="resfile.htm" />
    <None Remove="Resource_heading.jpg" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="resfile.htm" />
    <EmbeddedResource Include="Resource_heading.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.HTMLUI.Base" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.HTMLUI.Windows" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.MIME.Base" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Edit.Windows" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.SpellChecker.Base" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Grid.Windows" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Tools.Base" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Scripting.Base" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Scripting.Windows" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Shared.Base" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Shared.Windows" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Tools.Windows" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Licensing" Version="18.4.0.39" />
  </ItemGroup>
</Project>