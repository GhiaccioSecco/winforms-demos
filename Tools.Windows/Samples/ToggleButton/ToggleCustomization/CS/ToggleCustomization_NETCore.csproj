    <PackageReference Include="Syncfusion.Shared.Windows" Version="18.3.0.35" />
    <PackageReference Include="Syncfusion.Tools.Base" Version="18.3.0.35" />
    <PackageReference Include="Syncfusion.Tools.Windows" Version="18.3.0.35" />
    <PackageReference Include="Syncfusion.SpellChecker.Base" Version="18.3.0.35" />
    <PackageReference Include="Syncfusion.Licensing" Version="18.3.0.35" />
  </ItemGroup>
</Project>