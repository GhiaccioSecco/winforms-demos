    <PackageReference Include="Syncfusion.SfListView.WinForms" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.SpellChecker.Base" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Core.Winforms" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Grid.Windows" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Shared.Base" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Shared.Windows" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Tools.Base" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Tools.Windows" Version="18.4.0.39" />
