    <None Remove="Resources\**" />
    <None Remove="Test Files\**" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Content.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="BlackThemeconfig.xml" />
    <None Remove="config.xml" />
    <None Remove="logo.ico" />
  </ItemGroup>
