<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>BubbleVisualization</AssemblyName>
    <RootNamespace>BubbleVisualization</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="..\..\ShapeFiles\world1.dbf">
      <Link>world1.dbf</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="..\..\ShapeFiles\world1.shp">
      <Link>world1.shp</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <PackageReference Include="Syncfusion.Shared.Base" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Shared.Windows" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Maps.Windows" Version="18.4.0.39" />
    <PackageReference Include="Syncfusion.Licensing" Version="18.4.0.39" />
  </ItemGroup>
</Project>