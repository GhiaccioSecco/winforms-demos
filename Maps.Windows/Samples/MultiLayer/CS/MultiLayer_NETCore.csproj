<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>MapsDemo_2012</AssemblyName>
    <RootNamespace>MapsDemo_2012</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="..\..\ShapeFiles\Africa.dbf">
      <Link>Africa.dbf</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="..\..\ShapeFiles\Africa.shp">
      <Link>Africa.shp</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="..\..\ShapeFiles\australia.dbf">
      <Link>australia.dbf</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="..\..\ShapeFiles\australia.shp">
      <Link>australia.shp</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="..\..\ShapeFiles\continent.dbf">
      <Link>continent.dbf</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="..\..\ShapeFiles\continent.shp">
      <Link>continent.shp</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <PackageReference Include="Syncfusion.Maps.Windows" Version="18.3.0.35" />
    <PackageReference Include="Syncfusion.Shared.Base" Version="18.3.0.35" />
    <PackageReference Include="Syncfusion.Shared.Windows" Version="18.3.0.35" />
    <PackageReference Include="Syncfusion.Licensing" Version="18.3.0.35" />
  </ItemGroup>
</Project>